import java.io.*;
class expt1
{
public static void main(String args[])
{
int a[]={3,5,0,7},z1,z2;
try
{
z1=(a[0]+a[1])/a[2];
}
catch(ArithmeticException e)
{
System.out.println("Divide by zero");
}
try
{
z2=(a[0]+a[1])/a[7];
}
catch(ArrayIndexOutOfBoundsException e)
{
System.out.println("Array out of boundary exception");
}
}
}
