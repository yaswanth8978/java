import java.io.*;
import java.util.*;
class GP
{
int a=5,b=10;
void display1()
{
System.out.println(a+b);
}
}
class P extends GP
{
int e=15,f=20;
void display2()
{
System.out.println(e+f);
}
}
class C extends P
{
int c=40,d=45;
void display()
{
System.out.println(c+d);
}
}
class z1
{
public static void main(String args[])
{
C b1=new C();
b1.display();
b1.display1();
b1.display2();
}
}
